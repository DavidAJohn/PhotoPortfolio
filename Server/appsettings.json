{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoConnection": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "PhotoPortfolioDb"
  },
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "{DOMAIN}",
    "TenantId": "{TENANT ID}",
    "ClientId": "{SERVER API APP CLIENT ID}",
    "CallbackPath": "/signin-oidc"
  },
  "Prodigi": {
    "ApiUri": "https://api.sandbox.prodigi.com/v4.0/",
    "ApiHost": "api.sandbox.prodigi.com",
    "ApiKey": "{YOUR_PRODIGI_API_KEY}"
  },
  "AzureUpload": {
    "AzureStorageConnectionString": "{YOUR_AZURE_STORAGE_CONNECTION_STRING}",
    "AzureContainerUri": "{YOUR_AZURE_STORAGE_CONTAINER_URI}",
    "AzureContainerName": "{YOUR_AZURE_STORAGE_CONTAINER_NAME}",
    "FileUploadTypesAllowed": ".jpeg,.jpg,.png",
    "MaxFileUploadSize": 2097152,
    "MaxFileNameLength": 75 
  },
  "Stripe": {
    "SecretKey": "{YOUR_STRIPE_SECRET_KEY}",
    "PublishableKey": "{YOUR_STRIPE_PUBLISHABLE_KEY}",
    "WhSecret": "{WEBHOOK_SECRET_FOR_LOCAL_STRIPE_CLI_TESTING}"
  },
  "SitePreferencesId": "{GENERATED_GUID}",
  "AdminUserName": "{YOUR_AZURE_ADMIN_NAME}"
}
